@model IEnumerable<DomainModel.DTO.Supplier.SupplierListItem>


<table class="table table-hover table-striped table-sm">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SupplierID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupplierName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mobile)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductCount)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SupplierID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupplierName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mobile)
            </td>
            <td>
                <span class="btn btn-danger del" data-ajax="true" data-target-id="dvContent" data-action="Delete" data-id="@item.SupplierID">Deleted</span>
                <span class="btn btn-primary edit" data-ajax="true" data-target-id="dvContent" data-action="Update" data-id="@item.SupplierID">Edit</span>
                
            </td>
        </tr>
}
    </tbody>
</table>
